{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "beberlei/assert": "^3.3.3",
        "doctrine/dbal": "^4.4.1",
        "doctrine/doctrine-bundle": "^3.2.2",
        "doctrine/doctrine-fixtures-bundle": "^4.3.1",
        "doctrine/doctrine-migrations-bundle": "^3.7",
        "doctrine/orm": "^3.6.2",
        "dragonmantank/cron-expression": "^3.6",
        "phpdocumentor/reflection-docblock": "^5.6.6",
        "phpstan/phpdoc-parser": "^2.3.2",
        "symfony/asset": "^8.0.4",
        "symfony/asset-mapper": "^8.0.4",
        "symfony/console": "^8.0.4",
        "symfony/doctrine-messenger": "^8.0.5",
        "symfony/dotenv": "^8.0",
        "symfony/expression-language": "^8.0.4",
        "symfony/flex": "^2.10",
        "symfony/form": "^8.0.4",
        "symfony/framework-bundle": "^8.0.5",
        "symfony/http-client": "^8.0.5",
        "symfony/intl": "^8.0.4",
        "symfony/mailer": "^8.0.4",
        "symfony/messenger": "^8.0.4",
        "symfony/mime": "^8.0.5",
        "symfony/monolog-bundle": "^4.0.1",
        "symfony/notifier": "^8.0.4",
        "symfony/process": "^8.0.5",
        "symfony/property-access": "^8.0.4",
        "symfony/property-info": "^8.0.5",
        "symfony/runtime": "^8.0.1",
        "symfony/scheduler": "^8.0.4",
        "symfony/security-bundle": "^8.0.4",
        "symfony/serializer": "^8.0.5",
        "symfony/stimulus-bundle": "^2.32",
        "symfony/string": "^8.0.4",
        "symfony/translation": "^8.0.4",
        "symfony/twig-bundle": "^8.0.4",
        "symfony/uid": "^8.0.4",
        "symfony/ux-icons": "^2.32",
        "symfony/ux-turbo": "^2.32",
        "symfony/ux-twig-component": "^2.32",
        "symfony/validator": "^8.0.5",
        "symfony/web-link": "^8.0.4",
        "symfony/yaml": "^8.0.1",
        "symfonycasts/tailwind-bundle": "^0.12",
        "twig/extra-bundle": "^3.23.0",
        "twig/twig": "^3.23.0",
        "zenstruck/foundry": "^2.8.7"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.6",
        "friendsofphp/php-cs-fixer": "^3.93.1",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpstan": "^2.1.38",
        "phpstan/phpstan-beberlei-assert": "^2.0.2",
        "phpstan/phpstan-deprecation-rules": "^2.0.3",
        "phpstan/phpstan-doctrine": "^2.0.14",
        "phpstan/phpstan-phpunit": "^2.0.12",
        "phpstan/phpstan-symfony": "^2.0.12",
        "phpunit/phpunit": "^12.5.8",
        "rector/rector": "^2.3.5",
        "spaze/phpstan-disallowed-calls": "^4.7.1",
        "symfony/browser-kit": "^8.0.4",
        "symfony/css-selector": "^8.0",
        "symfony/debug-bundle": "^8.0",
        "symfony/maker-bundle": "^1.65.1",
        "symfony/stopwatch": "^8.0",
        "symfony/web-profiler-bundle": "^8.0.4",
        "symplify/phpstan-rules": "^14.9.11",
        "tomasvotruba/lines": "^1.2",
        "tomasvotruba/type-coverage": "^2.1",
        "vincentlanglet/twig-cs-fixer": "^3.13"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^8.0",
            "docker": true
        }
    }
}
